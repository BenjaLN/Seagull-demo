# Supabase Configuration
# Get these from your Supabase project settings at https://app.supabase.com
# Project Settings > API
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Development Mode
# Set to "true" to use mock data (no database required)
# Set to "false" to use real Supabase data
USE_MOCK=true

# Harbor Configuration
# The slug of the harbor to manage (must match slug in harbors table)
HARBOR_SLUG=rungsted-havn



